<h1>Create New Blogger</h1>
<%= form_for @blogger do |f| %>
<%= f.label :name %><br>
<%= f.text_field :name %><br>
<%= f.label :age %><br>
<%= f.text_field :age %><br>
<%= f.label :bio %><br>
<%= f.text_area :bio %><br>
<%= f.object.errors.full_messages.join(", ") if f.object.errors.any? %>
<%= f.submit %><br>
<% end %>
