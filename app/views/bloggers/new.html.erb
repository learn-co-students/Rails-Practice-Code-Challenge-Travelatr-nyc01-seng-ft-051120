<h1>Create Blogger!</h1>

<% if flash[:errors] %>
<ul>
<%flash[:errors].each do |error| %>
<li><%= error%></li>
<%end%>
</ul>
<%end%>

<%= form_for @blogger do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>

<%= f.label :age %>
<%= f.text_field :age %><br>

<%= f.label :bio %>
<%= f.text_area :bio %><br>

<%=f.submit%>
<%end%>